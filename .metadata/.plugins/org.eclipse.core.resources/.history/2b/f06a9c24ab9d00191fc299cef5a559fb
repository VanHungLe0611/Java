package familyTree;

import java.util.ArrayList;

public class FamilyTree {
	private ArrayList<FamilyMember> familyMembers = new ArrayList<FamilyMember>();
	
	public FamilyMember creatPerson(String firstName, String surname, int birthYear, boolean isMale) {
		familyMembers.add(new FamilyMember(firstName, surname, birthYear, isMale));
	}

}
